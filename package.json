{
  "name": "novadocs",
  "license": "MIT",
  "scripts": {
    "start": "node_modules/.bin/docsify serve .",
    "convert": "node_modules/.bin/docsify-pdf-converter"
  },
  "devDependencies": {
    "docsify-cli": "^4.3.0",
    "docsify-pdf-converter": "^2.0.7"
  },
  "docsifytopdf": {
    "contents": [
      "_whitepaper.md"
    ],
    "pdfOptions": {
      "displayHeaderFooter": true,
      "footerTemplate": "<b>Test</b>"
    },
    "pathToPublic": "whitepaper.pdf",
    "removeTemp": true,
    "emulateMedia": "screen"
  }
}
